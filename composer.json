{
    "name": "pixelmachine/craft-gpxcm",
    "description": "Campaign Monitor Service is an API wrapper and settings manager for Campaign Monitor plugins for Craft.",
    "type": "craft-plugin",
    "version": "2.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "email",
        "campaignmonitor",
        "campaign monitor"
    ],
    "support": {
        "docs": "https://github.com/pixelmachine/gpxcm",
        "issues": "https://github.com/pixelmachine/gpxcm"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jamie Grisdale",
            "homepage": "https://www.gp.works"
        }
    ],
    "require": {
        "craftcms/cms": "^3.4.0|^4.6.2",
        "campaignmonitor/createsend-php": "^6.1.2"
    },
    "autoload": {
        "psr-4": {
          "pixelmachine\\gpxcm\\": "src/"
        }
    },
    "extra": {
        "name": "GPxCM",
        "handle": "gpxcm",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://github.com/pixelmachine/gpxcm/blob/master/CHANGELOG.md",
        "class": "pixelmachine\\gpxcm\\GPxCM"
    }
}
